@model IEnumerable<OneMoreTime.Models.Funcionario>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table table-hover table-striped">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.FunNome)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FunEndereco)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FunTelefone)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FunCargo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FunCPF)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.FunNome)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FunEndereco)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FunTelefone)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FunCargo)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FunCPF)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.ID }) |
            @Html.ActionLink("Details", "Details", new { id=item.ID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.ID })
        </td>
    </tr>
}

</table>
